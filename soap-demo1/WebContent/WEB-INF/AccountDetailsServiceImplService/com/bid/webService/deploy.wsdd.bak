<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AccountDetailsServiceImplService WSDL service -->

  <service name="AccountDetailsServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webService.bid.com"/>
      <parameter name="wsdlServiceElement" value="AccountDetailsServiceImplService"/>
      <parameter name="schemaQualified" value="http://webService.bid.com"/>
      <parameter name="wsdlServicePort" value="AccountDetailsServiceImpl"/>
      <parameter name="className" value="com.bid.webService.AccountDetailsServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AccountDetailsServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getStatus" qname="operNS:getStatus" xmlns:operNS="http://webService.bid.com" returnQName="retNS:getStatusReturn" xmlns:retNS="http://webService.bid.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getStatus"/>

  </service>
</deployment>
